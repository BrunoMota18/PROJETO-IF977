<h1>Welcome to the RabbitCare!</h1>

<% if user_signed_in? %>
  <li>
  <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
  </li>
<% else %>
  <li>
  <%= link_to('Login', new_user_session_path)  %>  
  </li>
<% end %>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Nome:</strong>
  <%= @user.nome %>
</p>

<p>
  <strong>Tipo:</strong>
  <%= @user.tipo %>
</p>

<p>
  <strong>Tipo:</strong>
  <%= @user.crm %>
</p>